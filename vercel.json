{
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/__/auth/:path*",
      "destination": "https://master-text2voice.firebaseapp.com/__/auth/:path*"
    },
    {
      "source": "/((?!api/|__/auth/|.*\\.).*)",
      "destination": "/index.html"
    }
  ]
}